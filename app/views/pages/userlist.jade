extend ../layout.jade
block content
	.container
		.row
			table.table.table-hover.table-bordered
				thead
					tr
						th 用户名
						th 时间
						th 查看
						//- th 修改
						//- th 删除
				tbody
				each item in users
					tr
						th #{item.name}
						th #{moment(item.meta.updateAt).format('YYYY/MM/DD')};
						th #{item.password}	

